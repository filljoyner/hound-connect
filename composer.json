{
  "name": "filljoyner/hound",
  "description": "Fluent connector to Hound Service",
  "license": "MIT",
  "keywords": [
    "Hound"
  ],
  "authors": [
    {
      "name": "filljoyner",
      "email": "hello@filljoyner.com",
      "homepage": "https://filljoyner.com",
      "role": "Coffee Maker :)"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.*",
    "symfony/var-dumper": "2.*"
  },
  "autoload": {
    "psr-4": {
      "filljoyner\\Hound\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "filljoyner\\Hound\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "stable"
}
